{
  "name": "luyadev/luya-kickstarter-bootstrap4",
  "description": "LUYA Ecommerce Admin Template based on Bootstrap 4 .",
  "type": "luya-kickstarter",
  "license": "MIT",
  "require": {   
    "luyadev/luya-module-admin": "^5.0",
    "luyadev/luya-module-cms": "^5.0",
    "luyadev/luya-bootstrap4": "^1.0",   
    "luyadev/luya-generic": "^1.0",
    "luyadev/luya-module-gallery": "^1.0",
    "luyadev/luya-module-forms": "^1.8",
    "siripravi/luya-module-ecommerce": "^1.0.0",
    "siripravi/luya-module-shopcart": "^1.1.0",
    "siripravi/luya-module-forms": "^1.0.0",
    "siripravi/luya-module-authhelper": "^1.0.0",    
    "dev7ch/luya-ext-slick": "^1.1",
    "yiisoft/yii2-authclient": "~2.2.0",
    "voskobovich/yii2-linker-behavior": "^4.0",
    "luyadev/luya-module-userauth": "^1.1",
    "powerkernel/yii2-photoswipe": "~1.1.5",   
    "creocoder/yii2-nested-sets": "^0.9.0",
    "luyadev/luya-headless": "^2.10",
    "luyadev/luya-headless-cms-api": "dev-master",
    "exocet/yii2-bootstrap-material-design": "^2.3",
    "bizley/migration": "^4.4"
   
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.0",
    "yiisoft/yii2-gii": "^2.0",
    "luyadev/luya-deployer": "^1.0",
    "twbs/bootstrap": "^4.0",
    "unglue/client": "^1.5"
  },
  "config": {
    "platform-check": false,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "luyadev/luya-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
